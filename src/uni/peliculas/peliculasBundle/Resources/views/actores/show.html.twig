{% extends '::base.html.twig' %}

{% block body -%}
    <h1>actores</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ entity.nombre }}</td>
            </tr>
            <tr>
                <th>Foto</th>
                <td>{{ entity.foto }}</td>
            </tr>
            <tr>
                <th>Biografia</th>
                <td>{{ entity.biografia }}</td>
            </tr>
            <tr>
                <th>Pelicula</th>
                <td>{{ entity.pelicula }}</td>
            </tr>
            <tr>
                <th>Premios</th>
                <td>{{ entity.premios }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('actores') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('actores_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
